{
  "name": "streak-pong-server",
  "version": "1.0.0",
  "description": "Streak Pong socket server",
  "main": "index.js",
  "scripts": {
    "dev": "cd ./src && nodemon index.ts",
    "build": "tsc",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types"
  },
  "keywords": [
    "streak",
    "pong"
  ],
  "author": "Varchasvi Pandey",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.21",
    "express": "^4.19.1",
    "socket.io": "^4.7.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5"
  }
}
